<mvc:View
  controllerName="csrdreporting.controller.Main"
  xmlns:mvc="sap.ui.core.mvc"
  displayBlock="true"
  xmlns="sap.m"
  xmlns:controls="csrdreporting.formengine">
  <Page id="page" title="{i18n>title}">
    <VBox>
      <Button class="sapUiLargeMarginBottom" press="updateJSON" text="Update message from outside" />
      <controls:FormEngine schema="{vm>/schema}" state="{vm>/state}" />
    </VBox>
  </Page>
</mvc:View>
