{
  "_version": "1.49.0",
  "sap.app": {
    "id": "csrdreporting",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.9.5",
      "toolsId": "88ac33d3-f817-4ff5-88d3-32ceac8b73c8"
    },
    "dataSources": {
      "svc": {
        "uri": "/sap/opu/odata/sap/Z_00_T1_SS23_CSRD_SRV/",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.113.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {},
        "sap.ui.codeeditor": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "csrdreporting.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/FormBuilder.css"
        },
        {
          "uri": "css/FormEngine.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "csrdreporting.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "Main",
          "target": "Main",
          "pattern": ":?query:"
        },
        {
          "name": "FormBuilder",
          "target": "FormBuilder",
          "pattern": "form-builder"
        },
        {
          "name": "Questionnaire",
          "target": "Questionnaire",
          "pattern": "questionnaire/{formSchemaType}/customer/{customerId}/:?query:"
        },
        {
          "name": "CustomerManagement",
          "target": "CustomerManagement",
          "pattern": "management"
        }
      ],
      "targets": {
        "Main": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Main",
          "viewName": "Main"
        },
        "FormBuilder": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "FormBuilder",
          "viewName": "FormBuilder"
        },
        "Questionnaire": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Questionnaire",
          "viewName": "Questionnaire"
        },
        "CustomerManagement": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "CustomerManagement",
          "viewName": "CustomerManagement"
        }
      }
    },
    "rootView": {
      "viewName": "csrdreporting.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
