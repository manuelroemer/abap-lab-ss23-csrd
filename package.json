{
  "name": "csrd-reporting",
  "version": "0.0.1",
  "private": true,
  "description": "The CSRD Reporting application.",
  "main": "webapp/index.html",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "scripts": {
    "start": "npm-run-all --parallel watch watch:controls",
    "watch": "fiori run --open \"index.html?sap-ui-xx-viewCache=false\"",
    "watch:controls": "npx @ui5/ts-interface-generator --watch",
    "declaration": "npx @ui5/ts-interface-generator",
    "build": "npm run declaration && ui5 build --config=ui5.yaml --clean-dest --dest dist",
    "deploy": "npm run build && fiori deploy --config ui5-deploy.yaml && rimraf archive.zip",
    "deploy-config": "fiori add deploy-config",
    "undeploy": "fiori undeploy --config ui5-deploy.yaml",
    "tsc": "tsc --noEmit",
    "format": "prettier -w .",
    "lint": "eslint webapp",
    "prepare": "husky install && npm run declaration"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "2.2.0",
    "@sap/ux-ui5-tooling": "1.9.6",
    "@sapui5/types": "1.113.0",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "@ui5/cli": "3.1.2",
    "@ui5/ts-interface-generator": "0.5.5",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "typescript": "5.0.4",
    "ui5-tooling-transpile": "0.7.10",
    "yo": "4.3.1",
    "rimraf": "3.0.2",
    "ui5-task-flatten-library": "0.2.5"
  },
  "sapuxLayer": "CUSTOMER_BASE",
  "ui5": {
    "dependencies": [
      "ui5-task-flatten-library"
    ]
  }
}
